#include <iostream>
using namespace std;
class num
{
    public:
    int n,sum=0,i;
    num()
    {
        n=5;
        for( i=1;i<=n;i++)
        {
             sum=sum+i;
        }
    }
    void display()
    {
        cout<<"sum:"<<sum;
    }
};
int main() {
    num n1;
    n1.display();
    return 0;
}
