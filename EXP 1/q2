#include <iostream>
using namespace std;
class book
{
    public:
    string book_name;
    int book_price;
    int book_pages;
    void accept()
    {
        cout<<"enter book name,book price and no. of pages:";
        cin>>book_name>>book_price>>book_pages;
    }
    void display()
    {
        cout<<"book details of book with greater price are:";
        cout<<book_name<<endl<<book_price<<endl<<book_pages;
    }
};
int main() {
    book b1,b2;
    b1.accept();
    b2.accept();
    if(b1.book_price>b2.book_price)
    {
        b1.display();
    }
    else
    {
        b2.display();
    }

    return 0;
}
