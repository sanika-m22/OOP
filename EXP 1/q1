#include <iostream>
using namespace std;
class student
{
    private:
    int roll;
    string name;
    public:
    void accept()
    {
        cout<<"enter rollno. and name:";
        cin>>roll>>name;
    }
    void display()
    {
        cout<<"details are:"<<name<<" "<<roll;
    }
}s1;
int main() {
    s1.accept();
    s1.display();
    return 0;
}
